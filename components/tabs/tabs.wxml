<view class="tabs">
    <view class="tabs_title">
        <view bindtap="handleItem" data-index="{{index}}" class="title_item  {{item.isActive?'active':''}}"
            wx:for="{{tabs}}" wx:key="id">
            {{item.value}}
        </view>
    </view>
    <view class="tabs_content">
        <slot></slot>
    </view>
</view>