<view class="goods_list">
    <searchInput></searchInput>
    <tabs tabs="{{tabs}}" bindtabsHandleItem="handleItem">
        <block wx:if="{{tabs[0].isActive}}">
            <view class="first_tab">
                <navigator wx:for="{{goodsList}}" wx:key="goods_id" class="goods_item"
                    url="/pages/goods_detail/index?goods_id={{item.goods_id}}">
                    <!-- 左侧容器 -->
                    <view class="goods_img_wrap">
                        <image mode="widthFix"
                            src="{{item.goods_small_logo?item.goods_small_logo:'data:image/jpeg;base64,'}}">
                        </image>
                    </view>
                    <!-- 右侧容器 -->
                    <view class="goods_info_wrap">
                        <view class="goods_name">{{item.goods_name}}</view>
                        <view class="goods_price">{{item.goods_price}}</view>
                    </view>
                </navigator>
            </view>
        </block>
        <block wx:if="{{tabs[1].isActive}}">2</block>
        <block wx:if="{{tabs[2].isActive}}">3</block>
    </tabs>
</view>